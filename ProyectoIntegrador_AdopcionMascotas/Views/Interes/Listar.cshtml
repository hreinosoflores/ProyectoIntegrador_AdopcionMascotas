@model IEnumerable<Dominio.Entidades.Interes_Tuneado>

@{
    ViewBag.Title = "Interesados por mi mascota";
    Layout = "~/Views/Shared/_LayoutPerfil.cshtml";
}

<h2>@ViewBag.Title</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CODUSUARIO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.INTERESADO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHAINTERES)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CODUSUARIO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.INTERESADO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FECHAINTERES)
        </td>
        <td>
            
            @Html.ActionLink("Ver detalles", "Detalle","Usuario", new { codusuario=item.CODUSUARIO },null) 
       
        </td>
    </tr>
}

</table>
